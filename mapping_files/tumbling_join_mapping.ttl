@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rmls: <http://semweb.mmlab.be/ns/rmls#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix activity: <http://example.com/activity/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>. 
@base <http://example.com/base/> .

_:kafka_source_ndwSpeed a rmls:KafkaStream ;
             rmls:broker "192.168.0.237:9092";
             rmls:groupId "1";
             rmls:topic "ndwSpeed".

_:kafka_source_ndwFlow a rmls:KafkaStream ; 
            rmls:broker "192.168.0.237:9092"; 
            rmls:groupId "1"; 
            rmls:topic "ndwFlow". 



<JoinConfigMap> a rmls:JoinConfigMap;
        rmls:joinType rmls:TumblingWindowJoin;


<NDWSpeedMap>
  a rr:TriplesMap;

  rml:logicalSource [
    rml:source _:kafka_source_ndwSpeed;
    rml:referenceFormulation ql:JSONPath:
    rml:iterator "$"
  ];

  rr:subjectMap [
    rr:template "http://example.com/resource/{internalId}" ;
    rr:class <http://example.com/ontology/Speed>
  ];

  rr:predicateObjectMap [
    rr:predicate schema:latitude;
    rr:objectMap [ rml:reference "lat" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate schema:long;
    rr:objectMap [ rml:reference "long" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate rmls:latency;
    rr:objectMap [ rml:reference "latencyRef" ]
  ] ;
  

  rr:predicateObjectMap [
    rr:predicate schema:accuracy;
    rr:objectMap [ rml:reference "accuracy" ]
  ] ;
  
  
  rr:predicateObjectMap [
    rr:predicate schema:speed;
    rr:objectMap [ rml:reference "speed" ]
  ] ;
  
  rr:predicateObjectMap [
    rr:predicate <http://example.com/ontology/laneFlow> ;
    rr:objectMap [
      a rr:RefObjectMap ;
      rr:parentTriplesMap <NDWFlowMap>;
      rmls:joinConfig <JoinConfigMap>;
      rmls:windowType  rmls:Tumbling;
      rr:joinCondition [
        rr:child "internalId,lat,long" ;
        rr:parent "internalId,lat,long" ;
      ]
    ]
  ] .

<NDWFlowMap>
  a rr:TriplesMap;
  rml:logicalSource [
    rml:source _:kafka_source_ndwFlow;
    rml:referenceFormulation ql:JSONPath:
    rml:iterator "$"
  ];



  rr:subjectMap [
    rr:template "http://example.com/resource/{internalId}";
    rr:class <http://example.com/ontology/Flow>;
    rr:graph <http://example.com/graph/flow> ;
  ];
  

  rr:predicateObjectMap [
    rr:predicate rmls:latency;
    rr:objectMap [ rml:reference "latencyRef" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ex:accuracy;
    rr:objectMap [ rml:reference "accuracy" ];
  ];

  rr:predicateObjectMap [
    rr:predicate ex:period;
    rr:objectMap [ rml:reference "period" ];
  ];
  
  rr:predicateObjectMap [
    rr:predicate ex:flow;
    rr:objectMap [ rml:reference "flow" ];
  ].
